generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Memory {
  id         String   @id @default(cuid())
  userId     String
  latitude   Float
  longitude  Float
  anchor     Json
  text       String?
  photoUrl   String?
  audioUrl   String?
  createdAt  DateTime @default(now())

  @@index([latitude, longitude])
}
